{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "{pathSegment1?}/{pathSegment2?}/{pathSegment3?}/{pathSegment4?}/{pathSegment5?}/{pathSegment6}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/functions/catan-server.js"
}
