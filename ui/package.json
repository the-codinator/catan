{
  "name": "catan-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "check": "eslint --ext .js,.jsx,.ts,.tsx src",
    "fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "clean": "rm -rf build",
    "compile": "yarn run compile:nolint",
    "compile:nolint": "tsc",
    "postcompile": "yarn run fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile:nolint",
    "posttest": "yarn run check"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.22.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.2",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.3",
    "typescript": "^3.9.6"
  }
}
